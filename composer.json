{
    "name": "mnapoli/assembly",
    "type": "library",
    "description": "Implementation of interop container definitions",
    "license": "MIT",
    "version": "dev-master",
    "authors": [
        {
            "name": "Matthieu Napoli",
            "email": "matthieu@mnapoli.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Assembly\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Assembly\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "container-interop/container-interop": "~1.1",
        "container-interop/definition-interop": "~0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "container-interop/definition-interop-tests": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
