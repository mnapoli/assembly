{
    "name": "mnapoli/assembly",
    "type": "library",
    "description": "Implementation of interop container definitions",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthieu Napoli",
            "email": "matthieu@mnapoli.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Assembly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Assembly\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "container-interop/container-interop": "~1.1",
        "container-interop/definition-interop": "@dev"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/container-interop/definition-interop.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    }
}
